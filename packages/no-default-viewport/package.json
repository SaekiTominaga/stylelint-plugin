{
	"name": "stylelint-no-default-viewport",
	"version": "2.0.0",
	"description": "Disallow default viewport-percentage units",
	"keywords": [
		"stylelint-plugin"
	],
	"homepage": "https://github.com/SaekiTominaga/stylelint-plugin#readme",
	"bugs": {
		"url": "https://github.com/SaekiTominaga/stylelint-plugin/issues"
	},
	"license": "MIT",
	"author": "Saeki Tominaga",
	"files": [
		"dist/**/*.d.ts",
		"dist/**/*.js"
	],
	"type": "module",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SaekiTominaga/stylelint-plugin.git"
	},
	"scripts": {
		"prebuild": "rimraf dist/* -g",
		"build": "tsc",
		"lint": "eslint src/**/*.ts",
		"pretest": "tsc --noEmit",
		"test": "node src/index.test.ts & node --experimental-test-coverage --test src/util/*.test.ts"
	},
	"peerDependencies": {
		"stylelint": "^17.0.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
