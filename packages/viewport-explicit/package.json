{
	"name": "stylelint-viewport-explicit",
	"version": "1.0.0",
	"description": "Disallow the use of default viewport-percentage units",
	"keywords": [
		"stylelint-plugin"
	],
	"homepage": "https://github.com/SaekiTominaga/stylelint-plugin#readme",
	"bugs": {
		"url": "https://github.com/SaekiTominaga/stylelint-plugin/issues"
	},
	"license": "MIT",
	"author": "Saeki Tominaga",
	"files": [
		"dist/**/*.d.ts",
		"!dist/**/*.test.d.ts",
		"dist/**/*.js",
		"!dist/**/*.test.js"
	],
	"type": "module",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SaekiTominaga/stylelint-plugin.git"
	},
	"scripts": {
		"build": "tsc",
		"watch": "tsc -w",
		"lint": "eslint src/*.ts",
		"pretest": "npm run build",
		"test": "node dist/index.test.js & node --experimental-test-coverage --test dist/util/*.test.js"
	},
	"peerDependencies": {
		"stylelint": "^16.0.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
